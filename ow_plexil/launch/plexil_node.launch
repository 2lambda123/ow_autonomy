<!-- Launch the plexil_node and terminal selection node. -->

<launch>
  <arg name="plan" default="None"/>

  <node pkg="ow_plexil"
        name="plexil_node"
        type="plexil_node"
        args="$(arg plan)"
        output="screen">
  </node>
  <node pkg="ow_plexil"
        name="terminal_selection_node"
        type="terminal_selection_node"
        args="$(arg plan)"
        output="screen">
  </node>

  <node pkg="ow_plexil"
        name="identify_sample_location"
        type="identify_sample_location.py"
        output="screen">
  </node>

</launch>
