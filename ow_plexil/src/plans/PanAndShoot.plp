// Auxiliary plan for TakePanorama.  Pans to specified angle and takes
// a picture.

#include "plexil_defs.h"
#include "plan-interface.h"

PanAndShoot:
{
  In Real PanAngle;

  LibraryCall Pan (PanDegrees = PanAngle);
  LibraryCall CameraCapture (ExposureSecs = 0);
}
