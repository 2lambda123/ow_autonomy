// Perform one pass of a linear digging action.

#include "plan-interface.h"

DigLinear:
{
  In Real X;
  In Real Y;
  In Real Depth;
  In Real Length;
  In Real GroundPos;

  Exit Lookup(ArmFault);

  SynchronousCommand dig_linear (X, Y, Depth, Length, GroundPos);
}
