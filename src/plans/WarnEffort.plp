#include "OceanWorldDefs.h"

Command log_warning (...);

WarnEffort:
{
  In String joint;
  In Real effort;

  if (effort >= PEAK_EFFORT) {
    log_warning ("Peak effort in joint ", joint, ": ", effort);
  }
  else if (effort >= CRITICAL_EFFORT) {
    log_warning ("Critical effort in joint ", joint, ": ", effort);
  }
  else log_warning ("High effort in joint ", joint, ": ", effort);
  endif;
}
