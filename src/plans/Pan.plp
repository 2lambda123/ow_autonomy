// Antenna pan procedure.
// Input: degrees

#include "plan-interface.h"

Pan:
{
  In Real Degrees;
  Boolean started;
  PostCondition (! started);
  
  SynchronousCommand started = pan_antenna (Degrees);

  if (! started) {
    log_error ("Failed to start antenna pan to ", Degrees, " degrees.");
  }
  endif
}
