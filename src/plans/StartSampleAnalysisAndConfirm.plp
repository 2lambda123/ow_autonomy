// Wrapper to StartSampleAnalysis that adds a confirmation.

LibraryAction StartSampleAnalysis;

StartSampleAnalysisAndConfirm:
{
  InOut Boolean Confirm;
  LibraryCall StartSampleAnalysis();
  Confirm = true;
}
