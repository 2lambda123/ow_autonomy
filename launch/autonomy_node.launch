<!-- Launch the Autonomy module, which is a single node. -->

<launch>
  <param name="/use_sim_time" value="false"/>
  <param name="/use_onboard_target" value="true"/>
  <param name="/communications_delay" type="int" value="30"/>
  <param name="/decision_duration" type="int" value="36"/>
  <param name="/timeout_duration" type="int" value="110"/>
  <param name="/x_coordinate" type="double" value="6"/>
  <param name="/y_coordinate" type="double" value="7"/>
  <param name="/z_coordinate" type="double" value="0"/>
  <arg name="plan" default="Demo.plx"/>
  <node pkg="ow_autonomy"
        name="autonomy_node"
        type="autonomy_node"
        args="$(arg plan)"
        output="screen" />
  <node pkg="ow_autonomy"
        name="GuardedMoveServer"
        type="GuardedMoveServer"
        output="screen" />
  <node pkg="ow_autonomy"
        name="GroundControl"
        type="GroundControl"
        output="screen" />
</launch>
